      },
      "package": "com.bgrajales.thewatcherandroid",
      "versionCode": 3,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "App to keep track of movies and series watched, comment on them and follow new ones"
  }
