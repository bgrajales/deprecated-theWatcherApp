{
  "expo": {
    "name": "The Watcher",
    "slug": "theWatcherApp",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
